@using MyFlyer.Web.Models.DataModel;
@model List<MerchantViewModel>
    <ul class="main-nav nav navbar-nav">
        <li class="active"><a href="#">Home</a></li>
        @foreach (var merchant in Model)
        {
            <li><a asp-action="Details" asp-controller="Merchant" asp-route-id="@merchant.Id">@merchant.Name</a></li>            
        }
    </ul>